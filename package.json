    "allotment": "^1.20.5",
    "dagre": "^0.8.5",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.563.0",
    "monaco-editor": "^0.55.1",
    "node-pty": "^1.1.0",
    "react": "^18.3.1",
