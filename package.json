{
  "name": "js-blueprints-electron",
  "version": "0.5.1",
  "type": "module",
  "description": "A powerful visual programming editor for Javascript and Node.js blueprints.",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && tsc -p tsconfig.electron.json && shx mkdir -p dist/electron && shx cp electron/package.json dist/electron/package.json && shx cp -r electron/runners dist/electron/",
    "start": "electron .",
    "electron:dev": "concurrently -k \"cross-env VITE_CJS_IGNORE_WARNING=true BROWSER=none npm run dev\" \"wait-on tcp:5173 && tsc -p tsconfig.electron.json && shx mkdir -p dist/electron && shx cp electron/package.json dist/electron/package.json && shx cp -r electron/runners dist/electron/ && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "lint": "eslint .",
    "test": "vitest run",
    "security-audit": "npm audit",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch"
  },
  "keywords": [],
  "author": "DiegoSmk <guinho_p1@hotmail.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.29.0",
    "@babel/generator": "^7.29.0",
    "@babel/parser": "^7.28.6",
    "@babel/traverse": "^7.29.0",
    "@babel/types": "^7.28.6",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@monaco-editor/react": "^4.7.0",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.8",
    "@xyflow/react": "^12.10.0",
    "allotment": "^1.20.5",
    "dagre": "^0.8.5",
    "esbuild": "^0.27.2",
    "esbuild-register": "^3.6.0",
    "framer-motion": "^12.31.0",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.563.0",
    "monaco-editor": "^0.55.1",
    "node-pty": "^1.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.5.4",
    "react-virtualized-auto-sizer": "^2.0.2",
    "react-window": "^2.2.6",
    "recast": "^0.23.11",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/babel__parser": "^7.0.0",
    "@types/dagre": "^0.7.53",
    "@types/node": "^25.0.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-is-dev": "^3.0.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^28.0.0",
    "shx": "^0.4.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.46.4",
    "vite": "^5.3.1",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^2.1.9",
    "wait-on": "^7.2.0"
  }
}